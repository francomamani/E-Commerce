border-radius()
	-webkit-border-radius arguments
	-moz-border-radius arguments
	border-radius arguments



box-shadow(offset-x, args)
      got-offset-x offset-x
      -webkit-box-shadow offset-x args
      -moz-box-shadow offset-x args
      box-shadow offset-x args

body

	width 100%
	font-size 16px
	
nav
	background-color #000
	color #fff
	margin 1em
	padding 0.5em

#contenedor
	margin 0 auto
	background-color #fff
	width 90%
	min-height 300px 


.productos
	display none

.thumb
	width 20%
	display inline-block
	vertical-align top
	img
		width: 60px;
		height: 60px;
		border-radius(5px)
.nom
	width 55%
	display inline-block
	vertical-align top
	overflow hidden
.botones
	display inline-block
	vertical-align top
	text-align right
	width 20%
.fila
	background #fff
	border-radius(5px)
	box-shadow 1px 2px 5px #000
	padding 3px
	margin 3px
	&:hover
		background-color #ccc
.ProdImg
	width 40%
	display inline-block
	vertical-align top
	img 
		width 120px
		height 120px
		border-radius(5px)
.items
	width 40%
	display inline-block
	vertical-align top
	p
		font-weight bold
		display inline-block

.jumbotron
	font-size 16px

